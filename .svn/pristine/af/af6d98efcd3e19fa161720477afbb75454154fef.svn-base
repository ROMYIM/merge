<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
<meta charset="UTF-8" />
<title>test</title>
<script type="text/javascript" src="/js/jquery-3.2.1.min.js"></script>
<script type="text/javascript" src="js/jsonFormat.js?ver=1"></script> 
</head>
<body>
 	<table>
 		<tr>
 			<td>Code: <input type="text" id="code"/></td>
			<td id="sntd"><span id="sntext">SN</span>: <input type="text" id="sn"/></td>
			<td>Type: <input type="text" id="type" value="1"/></td>
			<td><button id="login" onclick="login()">login</button></td>
 		</tr>
 	</table>
</body>
<script type="text/javascript">
function login() {
	$.post("/testHttp", {
		action : "testHttp",
		type : $('#type').val(),
		code : $("#code").val(),
		sn : $("#sn").val(),			
	}, function(data) {
		alert(data);
	})
}
</script>
</html>